<%- include('includes/head.ejs') %>
<section>
    <div class="content has-text-centered"><br>
        <h2>Aquí se habla de nuestros hobbies</h2><br><br>
        <form action="/lab18/hobbies" method="POST" enctype="multipart/form-data">
            <input type="hidden" name="_csrf" value="<%= csrfToken %>" >
            <label for="nombre">Agrega uno de tus hobbies: </label> 
            <input type="text" id="nombre" name="nombre" placeholder="Pedro">
            <label for="descripcion">Descipción: </label>
            <input type="text" id="descripcion" name="descripcion" placeholder="Le gusta la piña">
            <label for="Imagen">Imagen: </label>
            <input type="file" name="imagen" required><br><br>
             <input type="submit" value="Enviar"> 
        </form> <br><br>
        <a href="/lab18/">Regresar a la pagina principal</a> 
</section>
